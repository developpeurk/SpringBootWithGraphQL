type Query{
    userWallets: [Wallet]
}

type Wallet{
    id: String,
    balance: Float,
    createdAt: Float,
    currency: Currency,
    WalletTransaction: [WalletTransaction]
}

type Currency{
    code: String,
    name: String,
    salePrice: Float,
    purchasePrice: Float
}

type WalletTransaction{
    id: Int,
    timestamp: Float,
    wallet: Wallet,
    amount: Float,
    type: String
}